<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Doctors.doc_profile"
    android:background="#ffffff">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:fontFamily="@font/joannab"
                android:layout_marginLeft="160dp"
                android:textColor="#000000"
                android:text="Profile"
                android:textSize="25dp">

            </TextView>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center">

            <androidx.cardview.widget.CardView
                android:layout_width="350dp"
                android:layout_height="112dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:elevation="20dp"
                app:cardBackgroundColor="#ffffff"
                app:cardCornerRadius="10dp">


                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="20dp"
                    android:src="@drawable/person">


                </ImageView>

                <TextView
                    android:id="@+id/n"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="120dp"
                    android:layout_marginTop="25dp"
                    android:fontFamily="@font/joannab"
                    android:text="Dr. name"
                    android:textColor="#000000"
                    android:textSize="22dp">

                </TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/e"
                    android:layout_marginLeft="120dp"
                    android:layout_marginTop="53dp"
                    android:fontFamily="@font/jonalight"
                    android:text="email"
                    android:textColor="#000000"
                    android:textSize="18dp">

                </TextView>

                <TextView
                    android:id="@+id/g"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="80dp"
                    android:layout_marginLeft="120dp"
                    android:textColor="#000000"
                    android:fontFamily="@font/jonalight"
                    android:layout_centerInParent="true"
                    android:textSize="18dp"
                    android:text="Good Morning/Afternoon/Evening" />



            </androidx.cardview.widget.CardView>


        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:layout_marginTop="20dp">


            <androidx.cardview.widget.CardView
                android:layout_width="350dp"
                android:layout_height="57dp"
                android:onClick="doc_visible_detail"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:elevation="15dp"
                android:layout_gravity="center"
                app:cardBackgroundColor="#ffffff"
                app:cardCornerRadius="10dp">


                <ImageView
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/hii_top"
                    android:src="@drawable/notification">


                </ImageView>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="90dp"
                    android:fontFamily="@font/jonalight"
                    android:layout_marginTop="13dp"
                    android:text="Visible Detail"
                    android:textColor="#000000"
                    android:textSize="20dp">

                </TextView>

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="310dp"
                    android:src="@drawable/arrow_right">


                </ImageView>



            </androidx.cardview.widget.CardView>


        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:layout_marginTop="5dp">



        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:layout_marginTop="5dp">


            <androidx.cardview.widget.CardView
                android:layout_width="350dp"
                android:layout_height="57dp"
                android:layout_marginLeft="5dp"
                android:onClick="contact_us"
                android:layout_marginTop="10dp"
                android:elevation="15dp"
                android:layout_gravity="center"
                app:cardBackgroundColor="#ffffff"
                app:cardCornerRadius="10dp">


                <ImageView
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/hii_top"
                    android:src="@drawable/contact_icon">


                </ImageView>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="90dp"
                    android:fontFamily="@font/jonalight"
                    android:layout_marginTop="13dp"
                    android:text="Contact Us"
                    android:textColor="#000000"
                    android:textSize="20dp">

                </TextView>

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="310dp"
                    android:src="@drawable/arrow_right">


                </ImageView>



            </androidx.cardview.widget.CardView>


        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:layout_marginTop="5dp">


            <androidx.cardview.widget.CardView
                android:layout_width="350dp"
                android:layout_height="57dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:onClick="help"
                android:elevation="15dp"
                android:layout_gravity="center"
                app:cardBackgroundColor="#ffffff"
                app:cardCornerRadius="10dp">


                <ImageView
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/hii_top"
                    android:src="@drawable/help_icon">


                </ImageView>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="90dp"
                    android:fontFamily="@font/jonalight"
                    android:layout_marginTop="13dp"
                    android:text="Help"
                    android:textColor="#000000"
                    android:textSize="20dp">

                </TextView>

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="310dp"
                    android:src="@drawable/arrow_right">


                </ImageView>



            </androidx.cardview.widget.CardView>


        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:layout_marginTop="5dp">


            <androidx.cardview.widget.CardView
                android:id="@+id/logout"
                android:layout_width="350dp"
                android:layout_height="57dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:elevation="15dp"
                android:layout_gravity="center"
                app:cardBackgroundColor="#ffffff"
                app:cardCornerRadius="10dp">


                <ImageView
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/hii_top"
                    android:src="@drawable/logout_icon">


                </ImageView>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="90dp"
                    android:fontFamily="@font/jonalight"
                    android:layout_marginTop="13dp"
                    android:text="Logout"
                    android:textColor="#000000"
                    android:textSize="20dp">

                </TextView>

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="310dp"
                    android:src="@drawable/arrow_right">


                </ImageView>



            </androidx.cardview.widget.CardView>


        </LinearLayout>



        <LinearLayout
            android:layout_width="340dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@drawable/hii_top"
            android:layout_marginLeft="25dp"
            android:gravity="center"
            android:layout_marginTop="90dp"
            android:layout_alignParentBottom="true">


            <ImageView
                android:layout_width="52dp"
                android:layout_height="42dp"
                android:onClick="doc_dashboard"
                android:layout_marginLeft="20dp"
                android:src="@drawable/home">
            </ImageView>

            <ImageView
                android:layout_width="53dp"
                android:layout_height="45dp"
                android:layout_marginLeft="35dp"
                android:onClick="btn_appointment"
                android:src="@drawable/appointment">
            </ImageView>

            <ImageView
                android:layout_width="52dp"
                android:layout_height="45dp"
                android:layout_marginLeft="35dp"
                android:src="@drawable/option">
            </ImageView>

            <ImageView
                android:layout_width="52dp"
                android:layout_height="50dp"
                android:layout_marginLeft="35dp"
                android:onClick="doc_profile"
                android:src="@drawable/profile">
            </ImageView>

        </LinearLayout>


    </LinearLayout>

</LinearLayout>